<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/dimen_10dp">

            <LinearLayout
                android:id="@+id/divLL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="gone">

                <ImageView

                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center"
                    android:background="@drawable/division"
                    android:backgroundTint="@color/colorPrimary"

                    />


                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/divNameTV"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_5dp"
                    android:layout_weight="1"

                    android:text="Division" />


                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/divName"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/socLL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_5dp"
                android:orientation="horizontal"
                android:visibility="gone">


                <ImageView
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center"
                    android:background="@drawable/supplier"
                    android:backgroundTint="@color/colorPrimary"

                    />

                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/socNameTV"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_5dp"
                    android:layout_weight="1"
                    android:text="Society" />


                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/socName"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/drGodownLL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_5dp"
                android:orientation="horizontal"
                android:visibility="gone">


                <ImageView

                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_gravity="center"
                    android:background="@drawable/dr_godown"
                    android:backgroundTint="@color/colorPrimary"

                    />

                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/drGodownNameTV"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_5dp"
                    android:layout_weight="1"
                    android:text="DR Godown" />


                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/drGodownName"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/salesLL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_5dp"
                android:orientation="horizontal"
                android:visibility="gone">

                <ImageView
                    android:layout_width="16dp"

                    android:layout_height="16dp"
                    android:layout_gravity="center"
                    android:background="@drawable/user_16"
                    android:backgroundTint="@color/colorPrimary"

                    />

                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/gone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_5dp"
                    android:layout_weight="1"
                    android:text="Salesman" />


                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/salesManTV"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ellipsize="end"
                    android:maxLines="1" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/inchargeLL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_5dp"
                android:orientation="horizontal"
                android:visibility="gone">

                <ImageView
                    android:layout_width="16dp"

                    android:layout_height="16dp"
                    android:layout_gravity="center"
                    android:background="@drawable/user_16"
                    android:backgroundTint="@color/colorPrimary"

                    />

                <com.cgg.twdinspection.common.custom.CustomFontTextView

                    android:id="@+id/inchargeNameTV"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"

                    android:text="Incharge" />


                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/inchargeName"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ellipsize="end"
                    android:maxLines="1" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/dateLL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimen_5dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="16dp"

                    android:layout_height="16dp"
                    android:layout_gravity="center"
                    android:background="@drawable/clock_16"
                    android:backgroundTint="@color/colorPrimary"

                    />

                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/insDate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen_5dp"
                    android:layout_weight="1"
                    android:text="Date of Inspection" />


                <com.cgg.twdinspection.common.custom.CustomFontTextView
                    android:id="@+id/dateTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
            </LinearLayout>


        </LinearLayout>

    </LinearLayout>
</layout>